_PATHS=binstubs:bin:~/bin
_PATHS=$_PATHS:/usr/local/bin:/usr/local/sbin
_PATHS=$_PATHS:/opt/local/bin:/opt/local/sbin
_PATHS=$_PATHS:~/.rbenv/bin
_PATHS=$_PATHS:/usr/local/share/npm/bin
_PATHS=$_PATHS:/usr/local/heroku/bin
_PATHS=$_PATHS:~/Go/bin
_PATHS=$_PATHS:/usr/local/opt/postgresql@9.4/bin

